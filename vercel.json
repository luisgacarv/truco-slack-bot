{
  "builds": [
    {
      "src": "api/slack/index.py",
      "use": "@vercel/python",
      "config": { "runtime": "python3.10" }
    },
    {
      "src": "api/gemini/index.py",
      "use": "@vercel/python",
      "config": { "runtime": "python3.10" }
    }
  ],
  "routes": [
    {
      "src": "/api/slack",
      "dest": "/api/slack/index.py"
    },
    {
      "src": "/api/gemini",
      "dest": "/api/gemini/index.py"
    }
  ]
}
